<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.order.dao.OrderInfoDao">

    <insert id="saveOrderInfo" parameterType="com.order.domain.OrderInfo">
        insert into order_info(order_id,product_id,product_name,product_quantity) values (#{orderId},#{productId},#{productName},#{productQuantity})
    </insert>

</mapper>

